"VertexlitGeneric"
{
	"$basetexture" "snowysnowtime/compass_plate1"
	"$additive" 1
	"$selfillum" 1
	$angle		 0
	$translate		 "[0 0]"
	$center		 "[.5 .5]"

	$nocull 1

	Proxies
	{
		vuth_Compass
		{
			resultVar $angle
		}
		TextureTransform
		{
			"translateVar" "$translate"
                		"rotateVar" "$angle"
                		"centerVar" "$center"
                		"resultVar" "$basetexturetransform"
		}
	}
}
