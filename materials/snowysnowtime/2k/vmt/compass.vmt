"UnlitGeneric"
{
	"$basetexture" "snowysnowtime/compass"
	"$additive" "1"
	
	$angle		 0
	$translate		 "[0 0]"
	$center		 "[.5 .5]"

	$nocull 1

	Proxies
	{
		drc_Compass
		{
			resultVar $angle
		}
		TextureTransform
		{
			"translateVar" "$translate"
                		"rotateVar" "$angle"
                		"centerVar" "$center"
                		"resultVar" "$basetexturetransform"
		}
	}
}
